<h1>Medical History</h1>
<table class="table table-dark">
  <thead>
  <tr>
    <th scope="col">BloodPressure</th>
    <th scope="col">Patient-Weight</th>
    <th scope="col">Lab-test</th>
    <th scope="col">Dieases</th>
    <th scope="col">Dieases-Details</th>
    <th scope="col">Test-Results</th>
  </tr>
  </thead>
  <% @patient.medical_records.each do |patients| %>
    <tbody>
    <tr>
      <td><%= patients.patient_bp %></td>
      <td><%= patients.patient_weight %></td>
      <td><%= patients.lab_test %>></td>
      <td><%= patients.dieases %></td>
      <td><%= patients.dieases_detail %>></td>
      <td><%= patients.lab_test_results %></td>
    </tr>
    </tbody>
  <% end %>
</table>
<h1>Current Medical Report</h1>
<h6>PatientBloodPressure:
  <%= @medical.patient_bp %></h6>
<h6>Patient-Weight:<%= @medical.patient_weight %></h6>
<h6>Lab-test:<%= @medical.lab_test %></h6>
<h6>Dieases:<%= @medical.dieases %></h6>
<h6>Dieases-Details:<%= @medical.dieases_detail %></h6>
<h6>Test-Results:<%= @medical.lab_test_results %></h6>
<br><br><br>
<div><%= link_to "Update Medical History", edit_patient_medical_record_path(@patient), class: "btn btn-info" %></div>