<h1>Appointments Page</h1>
<div class="card card p-3 bg-light card-body ">
    <%= simple_form_for [@patient, @appointment] do |f| %>
      <%= f.input :date %>
      <div class="d-flex justify-content-center"><%= f.submit "Update Appointment", class: "btn btn-success " %></div>
    <% end %>
</div>
